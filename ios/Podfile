# Example Podfile
platform :ios, '13.0'

# Use the correct Flutter SDK path
flutter_application_path = File.expand_path('..', __FILE__)

load File.join(flutter_application_path, 'packages', 'flutter_tools', 'bin', 'podhelper.rb')

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  # Ensure Flutter dependencies are included
  install_all_flutter_pods(flutter_application_path)
end
